	<key>keyEquivalent</key>
	<string>^~H</string>
	<key>name</key>
	<string>HTML Templates</string>
	<key>patterns</key>
	<array>
		<dict>
			<key>begin</key>
			<string>(&lt;)(TMPL_COMMENT)(&gt;)</string>
			<key>captures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.tag.end.html.html-template</string>
				</dict>
				<key>2</key>
				<dict>
					<key>name</key>
					<string>support.type.html.html-template</string>
				</dict>
				<key>3</key>
				<dict>
					<key>name</key>
					<string>punctuation.definition.tag.begin.html.html-template</string>
