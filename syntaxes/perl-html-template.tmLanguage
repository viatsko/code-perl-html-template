			<string>meta.tag.block.any.html.html-template</string>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>&lt;TMPL_.*?&gt;</string>
					<key>name</key>
					<string>invalid.illegal.bad-comments-or-CDATA.html</string>
				</dict>
				<dict>
					<key>match</key>
					<string>.*?</string>
					<key>name</key>
					<string>comment.block.html</string>
				</dict>
			</array>
		</dict>
		<dict>
			<key>include</key>
			<string>#tmpl-tags</string>
		</dict>
		<dict>
			<key>include</key>
			<string>text.html.basic</string>
		</dict>
	</array>
	<key>repository</key>
	<dict>
		<key>expression-perl-method</key>
		<dict>
			<key>match</key>
			<string>\b[a-zA-Z0-0-_]+(?=\(.*\))</string>
			<key>name</key>
			<string>entity.name.function.perl.expression.html.html-template</string>
		</dict>
		<key>number</key>
		<dict>
			<key>match</key>
			<string>\s+\d</string>
			<key>name</key>
			<string>constant.numeric</string>
		</dict>
		<key>string-double-quoted</key>
